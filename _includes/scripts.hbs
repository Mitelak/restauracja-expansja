{{#if useMenuScript}}
<script>
  (function () {
    var $tables = document.querySelectorAll("table");
    var $menuItems = document.querySelectorAll("#menu a");

    function getTable(selector) {
      return document.querySelector(selector);
    }

    function hideTables() {
      $tables.forEach(function (node) {
        return node.classList.add("hide-menu-table");
      });
    }

    function removeMenuItemsActiveClass() {
      $menuItems.forEach(function (node) {
        return node.classList.remove("active");
      });
    }

    $menuItems.forEach(function (node) {
      return node.addEventListener("click", function (event) {
        event.preventDefault();
        hideTables();
        removeMenuItemsActiveClass();
        var clickedItem = event.currentTarget;
        clickedItem.classList.add("active");
        var table = getTable("table" + clickedItem.getAttribute("href"));
        table.classList.remove("hide-menu-table");
      });
    });
    $menuItems[0].click();
  })();
</script>
{{/if}}
