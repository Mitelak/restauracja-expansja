  $(document).ready(function () {
    $table = $('table');
    $table.hide();
    $('.menu a').click(function (evt) {
      $this = $(this);
      var lista = $this.attr("href");
      var wybor = 'table' + lista;
      $table.hide();
      $(wybor).fadeIn();
      $('.menu a').removeClass('active');
      $this.addClass('active');
      evt.preventDefault();
    });

    $('.menu a:first').click();
  });
</script>
